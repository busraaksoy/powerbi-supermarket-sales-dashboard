	measure 'Region Subtitle' =
			
			VAR SelectedMeasureName =
			    MAX ( 'Select Measure'[Select Measure] )
			
			VAR MeasureValue =
			    SWITCH (
			        SelectedMeasureName,
			        "Sales", [Total Sales],         -- Assumes [Total Sales] responds to region filter
			        "Profit", [Total Profit],       -- Assumes [Total Profit] responds to region filter
			        "Quantity", [Total Quantity], -- Assumes [Total Quantity Sold] responds to region filter
			        "Discount", [Total Discount],   -- Assumes [Total Discount] responds to region filter
			        BLANK()
			    )
			
			VAR YoYGrowth =
			    SWITCH (
			        SelectedMeasureName,
			        "Sales", [Sales YOY Growth],    -- Assumes [Sales YOY Growth] responds to region filter
			        "Profit", [Profit YOY Growth],  -- Assumes [Profit YOY Growth] responds to region filter
			        "Quantity", [Quantity YOY Growth], -- Assumes [Quantity YOY Growth] responds to region filter
			        "Discount", [Discount YOY Growth], -- Assumes [Discount YOY Growth] responds to region filter
			        BLANK()
			    )
			
			    -- format the measure value
			VAR MeasureValueFormatted =
			    SWITCH (
			        TRUE(),
			        SelectedMeasureName = "Profit Margin", FORMAT ( MeasureValue, "0.0%" ),
			        ABS ( MeasureValue ) >= 1000, FORMAT ( MeasureValue / 1000, "0.#K" ),
			        FORMAT ( MeasureValue, "0" )
			    )
			
			VAR RegionName =
			    MAX ( 'DimLocation'[Region] ) -- This correctly captures the region from the current filter context
			
			RETURN
			IF (
			    ISBLANK(MeasureValue),
			    "Best performing Region: " & RegionName & UNICHAR(10) & "Select a measure",
			    "Best performing Region: " & RegionName & " " & MeasureValueFormatted & " (" &
			        IF(NOT ISBLANK(YoYGrowth), FORMAT(YoYGrowth, "+0%;-0%"), "N/A") & " YOY)"
			)
